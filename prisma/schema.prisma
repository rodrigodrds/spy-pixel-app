generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PixelTrack {
  id        String   @id @default(cuid())
  ipAddress String
  userAgent String
  referer   String?
  metadata  String?
  city      String?
  region    String?
  country   String?
  latitude  Float?
  longitude Float?
  isp       String?
  org       String?
  timestamp DateTime @default(now())
}